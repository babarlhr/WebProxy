{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProxySettings": {
    
    "Routes": {
      "OllamaRoute": {
        "ClusterId": "OllamaCluster",
        "Match": {
          "Path":  "/ollama"
        }
      }
    },

    "Clusters": {
      "OllamaCluster": {
        "Destinations": {
          "ollamaDestinations": {
            "Address": "http://127.0.0.1:11434/"
          }
        }
      }
    }
  }
}
